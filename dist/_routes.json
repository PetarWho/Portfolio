{
  "version": 1,
  "description": "Cloudflare Pages routing config for SPA",
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/favicon.ico",
    "/favicon.png",
    "/*.jpg",
    "/*.png",
    "/*.gif",
    "/*.svg",
    "/*.webp"
  ],
  "rules": [
    {
      "include": ["/*"],
      "exclude": [
        "/assets/*",
        "/favicon.ico",
        "/favicon.png"
      ],
      "path": "/index.html"
    }
  ]
}
